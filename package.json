{
  "name": "promptheroready",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "postinstall": "npm_config_optional=false npm install @rollup/rollup-linux-x64-gnu --optional --force",
    "dev": "vite",
    "build": "ROLLUP_NO_NATIVE=true npm_config_optional=false tsc -b && ROLLUP_NO_NATIVE=true npm_config_optional=false vite build",
    "build:vercel": "node scripts/build-vercel.js",
    "build:production": "set NODE_ENV=production && npm_config_optional=false tsc -b && npm_config_optional=false vite build",
    "build:staging": "set NODE_ENV=production && npm_config_optional=false tsc -b && npm_config_optional=false vite build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "preview": "vite preview",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist node_modules/.vite",
    "analyze": "npm run build && npx vite-bundle-analyzer dist",
    "deploy": "node scripts/deploy.js",
    "deploy:staging": "node scripts/deploy.js staging",
    "deploy:production": "node scripts/deploy.js production",
    "deploy:analyze": "node scripts/deploy.js production --analyze",
    "db:deploy": "node database/deploy-simple.js",
    "db:verify": "node database/verify-simple.js",
    "db:types": "supabase gen types typescript --project-id $SUPABASE_PROJECT_ID > src/types/database.ts",
    "db:reset": "supabase db reset",
    "db:status": "supabase status",
    "mobile:test": "npm run build && npm run preview",
    "mobile:optimize": "npm run build:production && npm run analyze"
  },
